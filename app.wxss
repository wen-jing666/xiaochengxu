/*公用底部导航栏*/
.footer{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 105rpx;
  background: #d32e33;
  z-index: 999;
}
.footer view{
  position: relative;
  float: left;
  width: 33%;
}
.footer view:nth-child(1) image{
  position: absolute;
  top: 25rpx;
  left: 50%;
  margin-left: -20.5rpx;
  width: 41rpx;
  height: 36rpx;
}
.footer view:nth-child(2) image{
  position: absolute;
  top: 25rpx;
  left: 50%;
  margin-left: -15rpx;
  width: 37rpx;
  height: 30rpx;
}
.footer view:nth-child(3) image{
  position: absolute;
  top: 25rpx;
  left: 50%;
  margin-left: -18.5rpx;
  width: 37rpx;
  height: 38rpx;
}
.footer view:nth-child(4) image{
  position: absolute;
  top: 25rpx;
  left: 50%;
  margin-left: -16.5rpx;
  width: 33rpx;
  height: 33rpx;
}
.footer view text{
  display: block;
  margin-top: 75rpx;
  font-size: 15rpx;
  text-align: center;
  color: #FFF;
}
.footer view.active text{
  color: #7c0005;
}

/*我觉得*/
